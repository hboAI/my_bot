<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <!-- Gazebo ROS Factory Plugin -->
    <plugin name="gazebo_ros_factory" filename="libgazebo_ros_factory.so"/>
  </world>
</sdf>
